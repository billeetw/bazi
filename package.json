{
  "name": "bazi-project",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:startup": "vite build --config vite.startup.config.ts",
    "build:main": "vite build --config vite.main.config.ts",
    "deploy:worker": "cd worker && npm run deploy",
    "deploy:pages": "wrangler pages deploy .",
    "deploy": "npm run deploy:worker && npm run deploy:pages"
  },
  "dependencies": {
    "framer-motion": "^11.11.17",
    "jose": "^5.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "typescript": "^5.6.3",
    "vite": "^7.3.1"
  }
}
